[user]
	email = willian@willianfernandes.com.br
	name = Willian Fernandes
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[color "diff"]
  plain = black
  meta = black
  frag = cyan reverse
  old = red
  new = green
  commit = yellow
  whitespace = normal red
[alias]
  sh = !git-sh
  ci = commit
  co = checkout
  b = branch
  st = status
  who = shortlog -s --
  fl = log -p
  lg = log --oneline --decorate --graph
  sb = status -sb
  dw = diff --word-diff
[core]
	editor = vim
	excludesfile = /home/vagrant/.gitignore
[help]
  autocorrect = 1
[branch]
  autosetuprebase = always
[push]
  default = current
